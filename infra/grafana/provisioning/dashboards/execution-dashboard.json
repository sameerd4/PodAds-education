{
  "uid": "podads-exec",
  "title": "PodAds â€“ Execution Dashboard",
  "tags": ["podads", "execution", "on-call"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 6,
  "refresh": "10s",
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "Request Rate (RPS)",
      "type": "stat",
      "gridPos": {"x": 0, "y": 0, "w": 5, "h": 6},
      "targets": [
        {
          "expr": "rate(ad_requests_total[1m])",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 2,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 0.1, "color": "red"}
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "textMode": "value_and_name",
        "text": {
          "titleSize": 14,
          "valueSize": 24
        }
      }
    },
    {
      "id": 2,
      "title": "Error Rate (%)",
      "type": "stat",
      "gridPos": {"x": 5, "y": 0, "w": 5, "h": 6},
      "targets": [
        {
          "expr": "sum(rate(ad_errors_total[1m])) / sum(rate(ad_requests_total[1m])) * 100",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 0.5, "color": "yellow"},
              {"value": 1, "color": "red"}
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "textMode": "value_and_name",
        "text": {
          "titleSize": 14,
          "valueSize": 24
        }
      }
    },
    {
      "id": 3,
      "title": "Fill Rate (%)",
      "type": "stat",
      "gridPos": {"x": 10, "y": 0, "w": 5, "h": 6},
      "targets": [
        {
          "expr": "(sum(rate(ad_decisions_total{outcome=\"fill\"}[1m])) / sum(rate(ad_decisions_total[1m])) * 100) or vector(0)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "red"},
              {"value": 70, "color": "yellow"},
              {"value": 85, "color": "green"}
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "textMode": "value_and_name",
        "text": {
          "titleSize": 14,
          "valueSize": 24
        }
      }
    },
    {
      "id": 4,
      "title": "p95 Decision Latency (ms)",
      "type": "stat",
      "gridPos": {"x": 15, "y": 0, "w": 5, "h": 6},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(ad_decision_latency_ms_bucket[1m])) by (le))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 1,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 150, "color": "yellow"},
              {"value": 300, "color": "red"}
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "textMode": "value_and_name",
        "text": {
          "titleSize": 14,
          "valueSize": 24
        }
      }
    },
    {
      "id": 5,
      "title": "System Health",
      "type": "stat",
      "gridPos": {"x": 20, "y": 0, "w": 4, "h": 6},
      "targets": [
        {
          "expr": "up{job=\"podads-api\"}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {"text": "DOWN", "color": "red"},
                "1": {"text": "UP", "color": "green"}
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "red"},
              {"value": 1, "color": "green"}
            ]
          }
        }
      },
      "options": {
        "graphMode": "none",
        "colorMode": "value",
        "textMode": "value_and_name",
        "text": {
          "titleSize": 14,
          "valueSize": 24
        }
      }
    },
    {
      "id": 6,
      "title": "Top Filter Rejections (Last 5m)",
      "type": "barchart",
      "gridPos": {"x": 0, "y": 6, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "sum(rate(ad_filters_applied_total{passed=\"false\"}[5m])) by (filter_name)",
          "legendFormat": "{{filter_name}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 3,
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "orientation": "horizontal"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": false
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
    {
      "id": 7,
      "title": "Errors by Pipeline Stage",
      "type": "barchart",
      "gridPos": {"x": 12, "y": 6, "w": 12, "h": 8},
      "targets": [
        {
          "expr": "sum(rate(ad_errors_total[5m])) by (stage)",
          "legendFormat": "{{stage}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 3,
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "orientation": "horizontal"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": false
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
    {
      "id": 8,
      "title": "Candidates per Request (Avg)",
      "type": "stat",
      "gridPos": {"x": 0, "y": 14, "w": 6, "h": 6},
      "targets": [
        {
          "expr": "rate(ad_candidates_processed_sum[5m]) / rate(ad_candidates_processed_count[5m])",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 1,
          "color": {
            "mode": "palette-classic"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 5, "color": "yellow"},
              {"value": 20, "color": "red"}
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "textMode": "value_and_name",
        "text": {
          "titleSize": 12,
          "valueSize": 20
        }
      }
    },
    {
      "id": 9,
      "title": "Stage Latency (p95)",
      "type": "barchart",
      "gridPos": {"x": 6, "y": 14, "w": 12, "h": 6},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(ad_stage_latency_ms_bucket[5m])) by (stage, le))",
          "legendFormat": "{{stage}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 1,
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "orientation": "horizontal"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 50, "color": "yellow"},
              {"value": 100, "color": "red"}
            ]
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": false
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
    {
      "id": 10,
      "title": "No-Fill Rate",
      "type": "stat",
      "gridPos": {"x": 18, "y": 14, "w": 6, "h": 6},
      "targets": [
        {
          "expr": "(sum(rate(ad_decisions_total{outcome=\"no_fill\"}[1m])) / sum(rate(ad_decisions_total[1m])) * 100) or vector(0)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 15, "color": "yellow"},
              {"value": 30, "color": "red"}
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "textMode": "value_and_name",
        "text": {
          "titleSize": 12,
          "valueSize": 20
        }
      }
    },
    {
      "id": 11,
      "title": "Request Rate (Last 30m)",
      "type": "timeseries",
      "gridPos": {"x": 0, "y": 20, "w": 12, "h": 6},
      "targets": [
        {
          "expr": "rate(ad_requests_total[1m])",
          "legendFormat": "Requests/sec",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 2,
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "lineWidth": 2,
            "showPoints": "never"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "calcs": ["last"],
          "values": ["last"]
        },
        "tooltip": {
          "mode": "single"
        }
      }
    },
    {
      "id": 12,
      "title": "Fill Rate (Last 30m)",
      "type": "timeseries",
      "gridPos": {"x": 12, "y": 20, "w": 12, "h": 6},
      "targets": [
        {
          "expr": "(sum(rate(ad_decisions_total{outcome=\"fill\"}[1m])) / sum(rate(ad_decisions_total[1m])) * 100) or vector(0)",
          "legendFormat": "Fill Rate %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "decimals": 1,
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "lineWidth": 2,
            "showPoints": "never"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "red"},
              {"value": 70, "color": "yellow"},
              {"value": 85, "color": "green"}
            ]
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "calcs": ["last"],
          "values": ["last"]
        },
        "tooltip": {
          "mode": "single"
        }
      }
    }
  ],
  "timepicker": {
    "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "liveNow": false,
  "weekStart": ""
}
